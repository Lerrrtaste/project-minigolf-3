[application]

config/name="Minigolf22"
run/main_scene="res://scenes/login/Login.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://global.gd"
Notifier="*res://helpers/notifier/Notifier.tscn"
Nakama="*res://addons/com.heroiclabs.nakama/Nakama.gd"
Networker="*res://helpers/networker/Networker.tscn"
MapStorage="*res://helpers/map_storage/MapStorage.tscn"
MapData="*res://data/map_data.gd"

[debug]

multirun/number_of_windows=2
multirun/window_distance=900
multirun/add_custom_args=true
multirun/first_window_args="dev_user_1"
multirun/other_window_args="dev_user_2"

[display]

mouse_cursor/custom_image="res://assets/ui/cursor.png"

[editor_plugins]

enabled=PoolStringArray( "res://addons/multirun/plugin.cfg" )

[global]

scale=false
inst=false
multi=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
